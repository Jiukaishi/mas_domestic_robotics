<?xml version="1.0"?>
<package format="2">
  <name>mdr_recognize_people_action</name>
  <version>1.0.0</version>
  <description>An action for recognizing people in a crowd</description>

  <author email="minh.nguyen@smail.inf.h-brs.de">Minh Nguyen</author>
  <maintainer email="robotics@inf.h-brs.de">MAS robotics</maintainer>

  <license>GPLv3</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rospy</build_depend>
  <build_depend>roslint</build_depend>
  <build_depend>actionlib</build_depend>
  <build_depend>actionlib_msgs</build_depend>
  <build_depend>mas_perception_msgs</build_depend>

  <exec_depend>rospy</exec_depend>
  <exec_depend>actionlib</exec_depend>
  <exec_depend>actionlib_msgs</exec_depend>
  <exec_depend>mas_execution_manager</exec_depend>
  <exec_depend>message_generation</exec_depend>
  <exec_depend>mas_perception_msgs</exec_depend>

  <export></export>
</package>
